!! ===========  Appearances  ============

URxvt.termName: rxvt-unicode
URxvt.internalBorder: 18
URxvt.cursorBlink: true
URxvt.scrollBar: false
URxvt.background: #2e3440
URxvt.foreground: #ffffff
URxvt.cursorBlink: true

!! ===========  Fonts prefferenes  ============

URxvt.font: xft:JetBrainsMono Nerd Font Mono:size=9:autohint=true,xft:JetBrainsMono Nerd Font Mono:size=11:antialias=false
URxvt.boldFont: xft:JetBrainsMono Nerd Font Mono:size=11:weight=bold
URxvt.letterSpace: -1

!! ============  Larger history limit  =============

URxvt.saveLines: 1000000

!! =============  Perl extensions  ==============

URxvt.perl-ext-common: default,matcher,font-size

!! ==============  Open urls in browser with Control-Click  ===============

URxvt.urlLauncher: /usr/bin/firefox
URxvt.matcher.button: C1

!! ===============  Change font size on Control-Plus/Minus  ==================

URxvt.keysym.Control-minus:       resize-font:smaller
URxvt.keysym.Control-plus:        resize-font:bigger
URxvt.keysym.Control-equal:       resize-font:reset

!! ===============  Urxvt keysyms  ==================

URxvt.keysym.Home:          \033[1~
URxvt.keysym.End:           \033[4~
URxvt.keysym.Control-Left:  \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C
URxvt.keysym.Control-Shift-V: eval:paste_clipboard
URxvt.keysym.Control-Shift-C: eval:selection_to_clipboard

!! ===============  Color schemes  =================

URxvt.color0: #3f4c5e
URxvt.color1: #BF616A
